[Device]
AccessProtocol=storage
DeviceMatch=usb:066f:8004;
Vendor=Jetflash MP3 Player, AKA NAPA LCD-HD, AKA Medion, AKA Tevion  and other

[Media]
InputFormats=audio/x-wav
OutputFormats=audio/mpeg;audio/x-wav;audio/x-ms-wma

[Playlist]
Formats=audio/x-mpegurl

[storage]
DriveType=sd_mmc
PlaylistPath=PLAYLIST/%File
AudioFolders=/

